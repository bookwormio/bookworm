{
  "expo": {
    "name": "bookworm.io",
    "slug": "bookworm",
    "scheme": "bookworm.io",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bookworm-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/bookworm-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bookworm-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "7d65960e-a8fb-46ff-8df9-2e57a6226322"
      }
    },
    "ios": {
      "bundleIdentifier": "com.rilescode.BookWorm",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select photos."
      }
    }
  }
}
