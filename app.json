{
  "expo": {
    "name": "BookWorm",
    "slug": "BookWorm",
    "scheme": "BookWorm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bookworm-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/bookworm-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bookworm-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
